name=input("Enter your name: ")
if (name[0] == 'a' or name[0] == 'A' or name[0] == 'b' or name[0] == 'B'):
    print(name+' should be in room AB')
elif (name[0] == 'c' or name[0] == 'C'):
    print(name+' should be in room CD')
else:
    y=name.find(" ")
    frst_ltr=name[y+1]
    if (frst_ltr== 'z' or frst_ltr=='Z'):
        print(name+' should be in room Z')
    else:
        print(name+' should be in room OTHER')
